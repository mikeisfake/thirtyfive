])\n\n  const renderRandoms = () => {\n    return (\n      randoms.map( movie => (\n        <Link to='/review'>\n          <RandomMovie\n            poster={movie.poster}\n            id={movie.id}\n            title={movie.title}\n            year={movie.year}\n            plot={movie.plot}\n            show={handleClick}\n          />\n        </Link>\n      ))\n    )\n  }\n\n  const handleClick = event => {\n    dispatch(setMovieFromDB(event.target.id));\n    window.scrollTo(0,0)\n    dispatch(getRandoms())\n  }\n\n  return (\n    <>\n      <div className=\"randoms flex\">\n        <h1>random movies</h1>\n        {renderRandoms()}\n      </div>\n    </>\n  )\n}\n\nexport default RandomMovieContainer\n","/Users/mike/Documents/Projects/Learn/thirtyfive/thirtyfive/src/redux/reducers/getRandomsReducer.js",[],"/Users/mike/Documents/Projects/Learn/thirtyfive/thirtyfive/src/components/RandomMovie.js",["141"],"/Users/mike/Documents/Projects/Learn/thirtyfive/thirtyfive/src/redux/actions/getRandoms.js",[],"/Users/mike/Documents/Projects/Learn/thirtyfive/thirtyfive/src/redux/actions/setMovieFromDB.js",[],"/Users/mike/Documents/Projects/Learn/thirtyfive/thirtyfive/src/redux/actions/setMovieFromAPI.js",[],"/Users/mike/Documents/Projects/Learn/thirtyfive/thirtyfive/src/components/Button.js",[],"/Users/mike/Documents/Projects/Learn/thirtyfive/thirtyfive/src/redux/actions/deleteReview.js",[],{"ruleId":"142","replacedBy":"143"},{"ruleId":"144","replacedBy":"145"},{"ruleId":"146","severity":1,"message":"147","line":2,"column":23,"nodeType":"148","messageId":"149","endLine":2,"endColumn":34},{"ruleId":"146","severity":1,"message":"150","line":1,"column":17,"nodeType":"148","messageId":"149","endLine":1,"endColumn":25},{"ruleId":"146","severity":1,"message":"151","line":4,"column":10,"nodeType":"148","messageId":"149","endLine":4,"endColumn":25},{"ruleId":"146","severity":1,"message":"152","line":11,"column":9,"nodeType":"148","messageId":"149","endLine":11,"endColumn":14},{"ruleId"